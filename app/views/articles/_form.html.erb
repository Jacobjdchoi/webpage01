<%= form_for(@article) do |f| %>
    <div>
        <label for="title">제목</label>
        <%= f.text_field :title %>
    </div>
    <div>
        <label for="pics">부수 사진(사진 추가시 등록후 사이즈 조정 페이지로 넘어감.)</label>
        <%= f.file_field :pics %>
    </div>
    <div>
        <label for="body">내용</label>
        <%= f.text_area :body, id: "newsBody" %>
    </div>
    <div>
        <%= f.submit "등록하기" %>
    </div>
<% end %>